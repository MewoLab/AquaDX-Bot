{
  "name": "@clansty/maibot-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "tsx dirfix.ts && next build",
    "start": "next start",
    "lint": "next lint",
