    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240919.0",
    "@iconify-json/twemoji": "^1.2.0",
    "@opennextjs/cloudflare": "https://pkg.pr.new/@opennextjs/cloudflare@81",
    "@types/node": "^20.8.3",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@unocss/postcss": "^0.63.4",
    "esbuild-loader": "^4.2.2",
    "json-loader": "^0.5.7",
    "postcss": "^8.4.47",
    "ts-loader": "^9.5.1",
    "tsx": "^4.17.0",
